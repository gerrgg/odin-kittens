<%= form_for(@kitten) do |f| %>
    <%= render 'shared/errors', object: f.object %>
    Name: <%= f.text_field :name %> <br>
    Age: <%= f.number_field :age %> <br>
    Cuteness: <%= f.number_field :cuteness %> <br>
    Softness: <%= f.number_field :softness %> <br>
    <%= f.submit class: 'btn btn-success mt-2'%>
<% end %>